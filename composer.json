{
    "name": "barchart/lumen-static-core",
    "description": "(Mostly) static sites built on top of Lumen.",
    "license": "MIT",
    "authors": [
        {
            "name": "Tom Sisk",
            "email": "tom.sisk@barchart.com"
        }
    ],
    "require": {
        "php": "^7.1.3|^8.2",
        "laravel/lumen-framework": "^6.0|^10.0",
        "vlucas/phpdotenv": "^3.3|^5.5",
        "illuminate/mail": "^6.0|^10.24",
        "illuminate/cookie": "^6.0|^10.24",
        "symfony/var-dumper": "^4.1|^6.3"
    },
    "autoload": {
        "psr-4": {
            "Barchart\\LumenStatic\\": "src"
        },
        "files": [
            "src/Support/helpers.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
